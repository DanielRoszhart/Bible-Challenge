        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CategoryActivity"
            android:label="@string/app_name" />
        <activity
            android:name=".InstructionsActivity"
            android:label="@string/app_name" />
        <activity
            android:name=".WhyQuestionActivity"
            android:label="@string/app_name" />
        <activity
            android:name=".ResultActivity"
            android:label="@string/app_name" />
        <activity android:name=".WhenQuestionActivity" />
        <activity
            android:name=".WhereQuestionActivity"
            android:label="@string/title_activity_where_question" />
        <activity
            android:name=".WhoQuestionActivity"
            android:label="@string/title_activity_who_question" />
        <activity
            android:name=".WhatQuestionActivity"
            android:label="@string/title_activity_what_question"></activity>
    </application>

</manifest>